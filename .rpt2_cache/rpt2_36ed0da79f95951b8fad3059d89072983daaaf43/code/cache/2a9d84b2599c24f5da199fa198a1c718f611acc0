  "code": "import { Compare, defaultCompare, swap } from \"../../util\";\r\nfunction partition(array, left, right, compareFn) {\r\n    const pivot = array[(right + left) / 2 | 0];\r\n    let [i, j] = [left, right];\r\n    while (i <= j) {\r\n        while (compareFn(array[i], pivot) === Compare.LESS_THAN)\r\n            i++;\r\n        while (compareFn(array[j], pivot) === Compare.BIGGER_THAN)\r\n            j--;\r\n        if (i <= j) {\r\n            swap(array, i, j);\r\n            i++;\r\n            j--;\r\n        }\r\n    }\r\n    return i;\r\n}\r\nfunction quick(array, left, right, compareFn) {\r\n    let index;\r\n    if (array.length > 1) {\r\n        index = partition(array, left, right, compareFn);\r\n        if (left < index - 1)\r\n            console.log('Lt ', left), quick(array, left, index - 1, compareFn);\r\n        if (index < right)\r\n            console.log('Rt ', right), quick(array, index, right, compareFn);\r\n    }\r\n    console.log('LR ', left, right);\r\n    return array;\r\n}\r\nexport function quickSort(array, compareFn = defaultCompare) {\r\n    return quick(array, 0, array.length - 1, compareFn);\r\n}\r\n//# sourceMappingURL=quick-sort.js.map",
