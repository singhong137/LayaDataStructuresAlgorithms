  "code": "export class Path {\r\n    constructor() {\r\n        this._lastOriX = 0;\r\n        this._lastOriY = 0;\r\n        this.paths = [];\r\n        this._curPath = null;\r\n    }\r\n    beginPath(convex) {\r\n        this.paths.length = 1;\r\n        this._curPath = this.paths[0] = new renderPath();\r\n        this._curPath.convex = convex;\r\n    }\r\n    closePath() {\r\n        this._curPath.loop = true;\r\n    }\r\n    newPath() {\r\n        this._curPath = new renderPath();\r\n        this.paths.push(this._curPath);\r\n    }\r\n    addPoint(pointX, pointY) {\r\n        this._curPath.path.push(pointX, pointY);\r\n    }\r\n    push(points, convex) {\r\n        if (!this._curPath) {\r\n            this._curPath = new renderPath();\r\n            this.paths.push(this._curPath);\r\n        }\r\n        else if (this._curPath.path.length > 0) {\r\n            this._curPath = new renderPath();\r\n            this.paths.push(this._curPath);\r\n        }\r\n        var rp = this._curPath;\r\n        rp.path = points.slice();\r\n        rp.convex = convex;\r\n    }\r\n    reset() {\r\n        this.paths.length = 0;\r\n    }\r\n}\r\nclass renderPath {\r\n    constructor() {\r\n        this.path = [];\r\n        this.loop = false;\r\n        this.convex = false;\r\n    }\r\n}\r\n//# sourceMappingURL=Path.js.map",
